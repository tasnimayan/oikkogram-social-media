{
  "name": "nextjs_social_media",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run gql-generate:schema && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "gql-generate:schema": "powershell -Command \"$env:NEXT_PUBLIC_HASURA_URL=(Get-Content .env | Where-Object {$_ -match '^NEXT_PUBLIC_HASURA_URL='} | ForEach-Object { ($_ -split '=')[1] }); $env:HASURA_ADMIN_SECRET=(Get-Content .env | Where-Object {$_ -match '^HASURA_ADMIN_SECRET='} | ForEach-Object { ($_ -split '=')[1] }); npx gql-tada generate schema $env:NEXT_PUBLIC_HASURA_URL --header=\\\"x-hasura-admin-secret:$env:HASURA_ADMIN_SECRET\\\"\""
  },
